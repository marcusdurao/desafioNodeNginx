<table width="100%" border="1">
    <tr>
        <td colspan="3" align="center" bgcolor="black">
            <img src="/img/fullCycle.png">
        </td>
    </tr>
    <tr>
        <td colspan="3">
            <h1>Lista de Alunos</h1>
        </td>
    </tr>
    <tr class="trSelect" >
        <td width="20%">Ação</td>
        <td width="20%">Código</td>
        <td width="60%">Nome</td>
    </tr>
    {{#data}}
        <tr>
            <td>
                <a href="/update/{{id}}"><img src="/img/Editar.png" class="btnAcao"></a>
                <a href="/deletar/{{id}}" class="deletar"><img src="/img/Deletar.png" class="btnAcao"></a>
            </td>
            <td>{{id}}</td>
            <td>{{name}}</td>
        </tr>
    {{/data}}
    <tr>
        <td colspan="3">
            <input type="button" onclick="location.href='inserir';" value="Adicionar Aluno" />
        </td>
    </tr>
</table>